<%- include("../partials/header") %>

<h1>Sign In</h1>

<form action="/session" method="POST">
  <div class="form-group">
    <input
      type="email"
      class="form-control"
      name="email"
      placeholder="Email">
  </div>

  <div class="form-group">
    <input
      type="password"
      class="form-control"
      name="password"
      placeholder="Password"
    >
  </div>

  <input type="submit" value="Submit" class="btn btn-outline-success">
</form>

<%- include("../partials/footer") %>